{
  "name": "@t-ecosystem/t-launcher",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm run server\" \"pnpm run client\"",
    "server": "cd server && node index.js",
    "client": "cd client && pnpm run dev",
    "install:deps": "pnpm install && cd server && pnpm install && cd ../client && pnpm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
